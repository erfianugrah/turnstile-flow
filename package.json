{
  "name": "turnstile-flow-worker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "cd frontend && npm run build && cd ..",
    "typecheck": "tsc --noEmit && cd frontend && npm run typecheck && cd ..",
    "deploy": "npm run typecheck && npm run build && wrangler deploy",
    "tail": "wrangler tail",
    "cf-typegen": "wrangler types",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:fraud": "playwright test tests/fraud-stress-test.spec.ts",
    "test:fraud:headed": "playwright test tests/fraud-stress-test.spec.ts --headed",
    "test:basic": "playwright test tests/form-submission.spec.ts tests/ephemeral-id.spec.ts"
  },
  "dependencies": {
    "hono": "^4.7.12",
    "pino": "^9.7.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250107.0",
    "@playwright/test": "^1.56.1",
    "typescript": "^5.9.3",
    "wrangler": "^4.47.0"
  }
}
